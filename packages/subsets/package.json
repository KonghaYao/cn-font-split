{
    "name": "@konghayao/cn-font-split",
    "version": "4.7.2",
    "description": "中文 web font 切割工具",
    "main": "dist/index.js",
    "browser": "dist/browser/index.js",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/KonghaYao/cn-font-split/tree/ts/packages/subsets"
    },
    "scripts": {
        "build": "rollup -c ",
        "dev": "rollup -c -w",
        "build:browser": "rollup -c rollup.config.browser.mjs ",
        "dev:browser": "rollup -c rollup.config.browser.mjs -w",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "prepublish": "tsc && pnpm build && pnpm build:browser && tsc"
    },
    "keywords": [
        "font",
        "converter",
        "performance",
        "wasm",
        "woff2",
        "ttf",
        "otf",
        "opentype-fonts",
        "font-subsetter",
        "font-subset"
    ],
    "author": "KonghaYao<dongzhongzhidong@qq.com>",
    "license": "Apache-2.0",
    "dependencies": {
        "@chinese-fonts/font-contours": "^1.0.0",
        "@chinese-fonts/wawoff2": "^2.0.3",
        "@japont/unicode-range": "^1.0.0",
        "@konghayao/harfbuzzjs": "^1.1.0",
        "byte-size": "^8.1.1",
        "fs-extra": "^10.1.0",
        "md5": "^2.3.0",
        "opentype.js": "^1.3.4",
        "os-locale": "^6.0.2",
        "tslog": "^4.8.2",
        "workerpool": "^6.5.0"
    },
    "devDependencies": {
        "@types/byte-size": "^8.1.0",
        "@types/md5": "^2.3.2",
        "@types/opentype.js": "^1.3.4",
        "@types/workerpool": "^6.4.1",
        "comlink": "^4.4.1",
        "path-browserify": "^1.0.1",
        "resolve": "^1.22.2",
        "rollup": "2.79.0",
        "vitest": "^0.33.0"
    }
}
